<h1>Blog</h1>
<ul>
    {% for post in collections.posts %}
        <li>
            <a href="{{ post.url }}">
                {{ post.data.title }}
                <em>{{ post.data.date | date("LLLL d, yyyy") }}</em>
            </a>
        </li>
    {% endfor %}
</ul>